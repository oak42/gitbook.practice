{
  "plugins": [
    "-lunr", "-search", "search-pro-ling",
    "chapter-fold",
    "expandable-chapters",
    "code",
    "todo",
    "splitter",
    "intopic-toc",
    "edit-link",
    "lightbox",
    "hide-element",
    "bottom-navigation"

  ],
  "pluginsConfig": {
